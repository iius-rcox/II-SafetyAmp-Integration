{
  "title": "SafetyAmp Status",
  "uid": "safetyamp-status",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "tags": ["safetyamp", "prometheus"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Sync In Progress",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "max_over_time(safetyamp_sync_in_progress[$__interval])"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "stat",
      "title": "Minutes Since Last Sync",
      "gridPos": { "h": 4, "w": 6, "x": 4, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "(time() - max_over_time(safetyamp_last_sync_timestamp_seconds[$__range])) / 60"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "red", "value": 60 } ] },
        "graphMode": "none",
        "orientation": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "min" }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Errors (last 1h)",
      "gridPos": { "h": 4, "w": 4, "x": 10, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "sum(increase(safetyamp_errors_total[1h]))"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "graphMode": "none",
        "orientation": "auto",
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "title": "Records Processed (last 1h)",
      "gridPos": { "h": 4, "w": 6, "x": 14, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "sum(increase(safetyamp_records_processed_total[1h]))"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "graphMode": "none",
        "orientation": "auto",
        "colorMode": "value"
      }
    },
    {
      "type": "timeseries",
      "title": "Sync Operations by Status (rate)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "sum by (operation, status) (rate(safetyamp_sync_operations_total[$__rate_interval]))",
          "legendFormat": "{{operation}} {{status}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Sync Duration (P50/P90/P95/P99)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "histogram_quantile(0.50, sum by (le) (rate(safetyamp_sync_duration_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p50"
        },
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "histogram_quantile(0.90, sum by (le) (rate(safetyamp_sync_duration_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p90"
        },
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "histogram_quantile(0.95, sum by (le) (rate(safetyamp_sync_duration_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p95"
        },
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "histogram_quantile(0.99, sum by (le) (rate(safetyamp_sync_duration_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Cache Items by Cache",
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 12 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "max by (cache) (safetyamp_cache_items_total)",
          "legendFormat": "{{cache}}"
        }
      ],
      "options": { "displayMode": "basic" }
    },
    {
      "type": "stat",
      "title": "Cache Staleness (minutes) by Cache",
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 12 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "(time() - max by (cache) (safetyamp_cache_last_updated_timestamp_seconds)) / 60",
          "legendFormat": "{{cache}}"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "orientation": "horizontal",
        "colorMode": "value"
      },
      "fieldConfig": { "defaults": { "unit": "min" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Changes by Operation/Status (increase)",
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 12 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "sum by (entity_type, operation, status) (increase(safetyamp_changes_total[$__rate_interval]))",
          "legendFormat": "{{operation}} {{status}} {{entity_type}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Errors by Type/Entity (increase)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 19 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "sum by (error_type, entity_type, source) (increase(safetyamp_errors_total[$__rate_interval]))",
          "legendFormat": "{{error_type}} {{entity_type}} {{source}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Records Processed by Sync Type (increase)",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 19 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "$datasource" },
          "expr": "sum by (sync_type) (increase(safetyamp_records_processed_total[$__rate_interval]))",
          "legendFormat": "{{sync_type}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    }
  ]
}
